{
    "name": "helsesjekk",
    "version": "1.0.0",
    "main": "dist/index.js",
    "license": "MIT",
    "packageManager": "yarn@3.3.1",
    "scripts": {
        "build": "tsc",
        "dev": "nodemon src/index.ts | pino-pretty",
        "lint": "eslint src/**/*.ts",
        "start:migrate": "yarn prisma:migrate && yarn start:prod",
        "start:prod": "node index.js",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate deploy"
    },
    "dependencies": {
        "@prisma/client": "4.11.0",
        "@slack/bolt": "^3.12.2",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "fastify": "^4.14.1",
        "node-cron": "^3.0.2",
        "pino": "^8.11.0",
        "prisma": "^4.11.0",
        "remeda": "^1.8.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@navikt/eslint-config-teamsykmelding": "^1.4.0",
        "@types/node": "^18.15.0",
        "@types/node-cron": "^3.0.7",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "@typescript-eslint/parser": "^5.54.1",
        "eslint": "^8.35.0",
        "eslint-plugin-import": "^2.27.5",
        "nodemon": "^2.0.21",
        "pino-pretty": "^9.4.0",
        "prettier": "^2.8.4",
        "prettier-plugin-prisma": "^4.10.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "root": true,
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint",
            "import"
        ],
        "rules": {
            "no-console": "warn",
            "import/order": [
                "warn",
                {
                    "newlines-between": "always"
                }
            ]
        }
    },
    "resolutions": {
        "@slack/socket-mode@^1.3.0": "patch:@slack/socket-mode@npm%3A1.3.2#./.yarn/patches/@slack-socket-mode-npm-1.3.2-e3725781bc.patch"
    }
}

        "dev": "nodemon --exec 'ncc run' src/index.ts",
        "lint": "eslint src/**/*.ts"
    },
    "dependencies": {
        "@slack/bolt": "^3.12.2",
        "dotenv": "^16.0.3",
        "pino": "^8.8.0"
    },
    "devDependencies": {
        "@navikt/eslint-config-teamsykmelding": "^1.4.0",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@vercel/ncc": "^0.36.0",
        "eslint": "^8.32.0",
        "nodemon": "^2.0.20",
        "pino-pretty": "^9.1.1",
        "prettier": "^2.8.3",
        "typescript": "^4.9.4"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "root": true,
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
